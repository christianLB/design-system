# Ensure consistent package installation
engine-strict=true
prefer-offline=true
legacy-peer-deps=true

# Configure package resolution
package-lock=true
shamefully-hoist=true

# Configure registry (uncomment if using a private registry)
# registry=https://registry.npmjs.org/

# @your-org:registry=https://registry.npmjs.org/

# //registry.npmjs.org/:_authToken=${NPM_TOKEN}
